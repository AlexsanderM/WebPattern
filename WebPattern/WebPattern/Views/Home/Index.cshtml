@model IEnumerable<EmployeeView>

@{
    ViewBag.Title = "Index";
}

<table>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.Id</td>
        <td>@item.FirstName</td>
        <td>@item.Age</td>
        <td><a asp-action="Details" asp-route-id="@item.Id">Detail</a></td>
    </tr>
    }
</table>
