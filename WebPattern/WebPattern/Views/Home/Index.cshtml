@model IEnumerable<WebPattern.Models.EmployeeView>
@{
    Layout = null;
}

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.FirstName</td>
            <td>@item.Age</td>
        </tr>
        }
    </table>
</body>
</html>
